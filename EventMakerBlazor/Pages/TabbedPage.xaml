<?xml version="1.0" encoding="utf-8" ?>
<TabbedPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:local="clr-namespace:EventMakerBlazor"
             xmlns:android="clr-namespace:Microsoft.Maui.Controls.PlatformConfiguration.AndroidSpecific;assembly=Microsoft.Maui.Controls"
             android:TabbedPage.ToolbarPlacement="Bottom"
             x:Class="EventMakerBlazor.TabbedPage">

    <NavigationPage Title="One" IconImageSource="{StaticResource EventIcon}">
        <x:Arguments>
            <local:HomePage/>
        </x:Arguments>        
    </NavigationPage>
    <NavigationPage Title="Account" IconImageSource="{StaticResource AccountIcon}">
        <x:Arguments>
            <local:MainPage/>
        </x:Arguments>
    </NavigationPage>
    
</TabbedPage>